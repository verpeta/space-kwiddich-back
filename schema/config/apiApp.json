{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "id": "/apiApp",
    "type": "object",
    "required": [
        "routeDirPaths",
        "port"
    ],
    "additionalProperties": false,
    "properties": {
        "routeDirPaths": {
            "type": "array",
            "items": {
                "type": "string"
            }
        },
        "port": {
            "type": "integer",
            "minimum": 1,
            "maximum": 65535
        }
      }
}